   "source": [
    "def process_feedback(feedback_data):\n",
    "    for feedback in feedback_data:\n",
    "        # Basit örnek olarak pozitif/negatif geri bildirimi sayabiliriz\n",
    "        if feedback['rating'] >= 4:\n",
    "            print(\"Positive feedback for\", feedback['url'])\n",
    "        else:\n",
    "            print(\"Negative feedback for\", feedback['url'])\n"
   ]
