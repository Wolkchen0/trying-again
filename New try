  {
   "cell_type": "code",
   "execution_count": null,
   "id": "47a30291-f096-4d4e-97f7-919370804c55",
   "metadata": {},
   "outputs": [],
   "source": [
    "from prometheus_client import start_http_server, Summary\n",
    "\n",
    "# Performans ölçümünü başlat\n",
    "REQUEST_TIME = Summary('request_processing_seconds', 'Time spent processing request')\n",
    "\n",
    "@REQUEST_TIME.time()\n",
    "def process_request(task, url):\n",
    "    result = ai_decision(task, url)\n",
    "    return result\n",
    "\n",
    "# Sunucuyu başlat\n",
    "start_http_server(8000)\n",
    "\n",
    "# Örnek işlem\n",
    "task = \"browse\"\n",
    "url = \"https://example.com\"\n",
    "process_request(task, url)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "f82cd029-9768-4444-836c-31ea39afbf22",
   "metadata": {},
   "outputs": [],
   "source": [
    "def evaluate_performance(data):\n",
    "    correct_predictions = sum(d['feedback'] for d in data)\n",
    "    total_predictions = len(data)\n",
    "    accuracy = correct_predictions / total_predictions\n",
    "    print(f\"Model Accuracy: {accuracy}\")\n",
    "\n",
    "# Sonuçları değerlendir\n",
    "with open('logs.json') as f:\n",
    "    logs = json.load(f)\n",
    "evaluate_performance(logs)\n"
   ]
  },
