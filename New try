   "cell_type": "code",
   "execution_count": null,
   "id": "7b694dba-caf1-4d61-8fd8-c79fa1b3fec7",
   "metadata": {},
   "outputs": [],
   "source": [
    "import json\n",
    "import os\n",
    "\n",
    "def log_result(task, url, result):\n",
    "    log_data = {\"task\": task, \"url\": url, \"result\": result}\n",
    "    if not os.path.exists('logs.json'):\n",
    "        with open('logs.json', 'w') as f:\n",
    "            json.dump([], f)\n",
    "    with open('logs.json', 'r+') as f:\n",
    "        logs = json.load(f)\n",
    "        logs.append(log_data)\n",
    "        f.seek(0)\n",
    "        json.dump(logs, f)\n",
    "\n",
    "# Kullanıcıdan görev ve URL al\n",
    "task = input(\"Enter task (e.g., browse, news, images): \")\n",
    "url = input(\"Enter URL to browse or get news from: \")\n",
    "result = ai_decision(task, url)\n",
    "print(\"Result:\", result)\n",
    "\n",
    "# Sonucu kaydet\n",
    "log_result(task, url, result)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "47dbf7b6-4ffd-4e8c-ab53-8e2667a875bf",
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.feature_extraction.text import CountVectorizer\n",
    "from sklearn.linear_model import LogisticRegression\n",
    "\n",
    "# Örnek veri: task, url, result\n",
    "data = [\n",
    "    {\"task\": \"browse\", \"url\": \"https://example.com\", \"result\": \"Example Domain\"},\n",
    "    {\"task\": \"news\", \"url\": \"https://news.ycombinator.com/\", \"result\": \"Hacker News\"},\n",
    "    # Daha fazla veri ekleyin\n",
    "]\n",
    "\n",
    "# Eğitim verilerini hazırlayın\n",
    "texts = [d[\"url\"] for d in data]\n",
    "results = [d[\"result\"] for d in data]\n",
    "\n",
    "vectorizer = CountVectorizer()\n",
    "X = vectorizer.fit_transform(texts)\n",
    "model = LogisticRegression()\n",
    "model.fit(X, results)\n",
    "\n",
    "# Yeni tahminler\n",
    "new_url = \"https://example.com\"\n",
    "new_task = \"browse\"\n",
    "new_X = vectorizer.transform([new_url])\n",
    "predicted_result = model.predict(new_X)[0]\n",
    "print(\"Predicted Result:\", predicted_result)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "57218af0-9a3a-4f1b-b333-462673a0890a",
   "metadata": {},
   "outputs": [],
   "source": [
    "def collect_feedback(task, url, result):\n",
    "    feedback = input(f\"Was the result for task '{task}' and url '{url}' satisfactory? (yes/no): \")\n",
    "    return feedback.lower() == 'yes'\n",
    "\n",
    "feedback = collect_feedback(task, url, result)\n",
    "print(\"Feedback collected:\", feedback)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "673d9256-eb5f-4386-a430-f2b4154ffac9",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Yeni veri toplandıktan sonra\n",
    "new_data = {\"task\": task, \"url\": url, \"result\": result, \"feedback\": feedback}\n",
    "\n",
    "# Veri kümesine yeni veriyi ekle\n",
    "data.append(new_data)\n",
    "\n",
    "# Modeli yeniden eğit\n",
    "texts = [d[\"url\"] for d in data]\n",
    "results = [d[\"result\"] for d in data]\n",
    "X = vectorizer.fit_transform(texts)\n",
    "model.fit(X, results)\n"
   ]
