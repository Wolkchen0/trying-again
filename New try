    "import json\n",
    "\n",
    "def save_to_file(data, filename='data.json'):\n",
    "    with open(filename, 'w') as f:\n",
    "        json.dump(data, f)\n",
    "\n",
    "# Kullanıcıdan görev ve URL al\n",
    "task = input(\"Enter task (e.g., browse, news, images): \")\n",
    "url = input(\"Enter URL to browse, get news from, or get images from: \")\n",
    "\n",
    "if task == \"images\":\n",
    "    result = get_images(url)\n",
    "elif task == \"content\":\n",
    "    result = get_page_content(url)\n",
    "else:\n",
    "    result = ai_decision(task, url)\n",
    "\n",
    "print(\"Result:\", result)\n",
    "\n",
    "# Veriyi dosyaya kaydet\n",
    "save_to_file(result)\n"
   ]
