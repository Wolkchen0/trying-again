    "from sklearn.ensemble import IsolationForest\n",
    "\n",
    "def detect_anomalies(data):\n",
    "    model = IsolationForest(contamination=0.1)\n",
    "    data_points = [[d['metric1'], d['metric2']] for d in data]\n",
    "    predictions = model.fit_predict(data_points)\n",
    "    return predictions\n",
    "\n",
    "# Örnek anomali tespiti\n",
    "data = [\n",
    "    {'metric1': 10, 'metric2': 5},\n",
    "    {'metric1': 11, 'metric2': 6},\n",
    "    {'metric1': 50, 'metric2': 50},  # Anomalous data point\n",
    "]\n",
    "anomalies = detect_anomalies(data)\n",
    "print(\"Anomalies detected:\", anomalies)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "42e519b3-2a3f-4c8e-9b26-a118e65af148",
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.model_selection import cross_val_score\n",
    "\n",
    "def meta_learning(models, X, y):\n",
    "    best_score = -1\n",
    "    best_model = None\n",
    "    for model in models:\n",
    "        score = cross_val_score(model, X, y, cv=5).mean()\n",
    "        if score > best_score:\n",
    "            best_score = score\n",
    "            best_model = model\n",
    "    return best_model\n",
    "\n",
    "# Örnek model seçimi\n",
    "models = [LogisticRegression(), DecisionTreeClassifier(), RandomForestClassifier()]\n",
    "best_model = meta_learning(models, X, y)\n",
    "print(\"Best model selected:\", best_model)\n"
   ]
  },
