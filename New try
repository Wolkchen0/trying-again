   "source": [
    "def personalized_recommendation(user_id):\n",
    "    profile = user_profiles.get(user_id, {'positive': 0, 'negative': 0})\n",
    "    if profile['positive'] > profile['negative']:\n",
    "        return \"We recommend more content similar to your positive feedback.\"\n",
    "    else:\n",
    "        return \"We recommend trying new types of content.\"\n",
    "\n",
    "# Kullanıcı bazlı öneri örneği\n",
    "print(personalized_recommendation(user_id))\n"
   ]
  },
