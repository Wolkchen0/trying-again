    "import random\n",
    "\n",
    "def ab_test(task, url):\n",
    "    # Rastgele A veya B modelini seç\n",
    "    model_version = random.choice(['A', 'B'])\n",
    "    if model_version == 'A':\n",
    "        result = ai_decision(task, url)\n",
    "    else:\n",
    "        result = ai_decision_v2(task, url)  # Alternatif bir model\n",
    "    return model_version, result\n",
    "\n",
    "# A/B test sonucu\n",
    "task = \"browse\"\n",
    "url = \"https://example.com\"\n",
    "model, result = ab_test(task, url)\n",
    "print(f\"Model {model} result: {result}\")\n"
   ]
