   "cell_type": "code",
   "execution_count": null,
   "id": "9b19cccc-1417-4514-81f5-ab2ada19dc01",
   "metadata": {},
   "outputs": [],
   "source": [
    "user_roles = {\n",
    "    \"admin\": [\"access_all\"],\n",
    "    \"user\": [\"access_limited\"]\n",
    "}\n",
    "\n",
    "def check_access(user_role, resource):\n",
    "    permissions = user_roles.get(user_role, [])\n",
    "    if \"access_all\" in permissions or resource in permissions:\n",
    "        return True\n",
    "    return False\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "7cbc18a5-6f80-434d-a42e-b8a13ffb8d8a",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Hücre 1: Gerekli Kütüphaneler\n",
    "import requests\n",
    "from bs4 import BeautifulSoup\n",
    "import json\n",
    "import matplotlib.pyplot as plt\n",
    "import ipywidgets as widgets\n",
    "from IPython.display import display\n",
    "import logging\n",
    "from sklearn.feature_extraction.text import CountVectorizer\n",
    "from sklearn.linear_model import LogisticRegression\n",
    "from sklearn.metrics import accuracy_score\n",
    "# Daha fazla kütüphane ekleyin\n",
    "\n",
    "# Hücre 2: AI Modeli Fonksiyonları\n",
    "def ai_decision(task, url):\n",
    "    task = task.strip().lower()\n",
    "    if task == \"browse\":\n",
    "        return browse_internet(url)\n",
    "    elif task == \"news\":\n",
    "        return get_news_headlines(url)\n",
    "    else:\n",
    "        return \"Unrecognized task. Please enter 'browse' or 'news'.\"\n",
    "\n",
    "def browse_internet(url):\n",
    "    response = requests.get(url)\n",
    "    soup = BeautifulSoup(response.text, 'html.parser')\n",
    "    return soup.title.string\n",
    "\n",
    "def get_news_headlines(url):\n",
    "    response = requests.get(url)\n",
    "    soup = BeautifulSoup(response.text, 'html.parser')\n",
    "    headlines = soup.find_all('h2')\n",
    "    return [headline.get_text() for headline in headlines]\n",
    "\n",
    "# Hücre 3: Veri Toplama ve Güncelleme Fonksiyonları\n",
    "def log_result(task, url, result):\n",
    "    log_data = {\"task\": task, \"url\": url, \"result\": result}\n",
    "    if not os.path.exists('logs.json'):\n",
    "        with open('logs.json', 'w') as f:\n",
    "            json.dump([], f)\n",
    "    with open('logs.json', 'r+') as f:\n",
    "        logs = json.load(f)\n",
    "        logs.append(log_data)\n",
    "        f.seek(0)\n",
    "        json.dump(logs, f)\n",
    "\n",
    "def update_model():\n",
    "    texts = [d[\"url\"] for d in data]\n",
    "    results = [d[\"result\"] for d in data]\n",
    "    X = vectorizer.fit_transform(texts)\n",
    "    model.fit(X, results)\n",
    "    accuracy = accuracy_score(y, model.predict(X))\n",
    "    print(\"Model Accuracy:\", accuracy)\n",
    "\n",
    "# Hücre 4: Geri Bildirim ve Kullanıcı Arayüzü\n",
    "def collect_feedback(task, url, result):\n",
    "    feedback = input(f\"Was the result for task '{task}' and url '{url}' satisfactory? (yes/no): \")\n",
    "    return feedback.lower() == 'yes'\n",
    "\n",
    "task_input = widgets.Text(description='Task:', value='browse')\n",
    "url_input = widgets.Text(description='URL:', value='https://example.com')\n",
    "run_button = widgets.Button(description='Run')\n",
    "output_area = widgets.Textarea(description='Result:', value='')\n",
    "\n",
    "def run_task(b):\n",
    "    task = task_input.value.strip().lower()\n",
    "    url = url_input.value.strip()\n",
    "    result = ai_decision(task, url)\n",
    "    output_area.value = str(result)\n",
    "    log_result(task, url, result)\n",
    "\n",
    "run_button.on_click(run_task)\n",
    "\n",
    "display(task_input, url_input, run_button, output_area)\n",
    "\n",
    "# Hücre 5: Performans İzleme ve Değerlendirme\n",
    "def evaluate_performance(data):\n",
    "    correct_predictions = sum(d['feedback'] for d in data)\n",
    "    total_predictions = len(data)\n",
    "    accuracy = correct_predictions / total_predictions\n",
    "    print(f\"Model Accuracy: {accuracy}\")\n",
    "\n",
    "# Hücre 6: Gerçek Zamanlı ve İleri Düzey Özellikler\n",
    "import asyncio\n",
    "import websockets\n",
    "\n",
    "async def communicate():\n",
    "    uri = \"ws://localhost:8765\"\n",
    "    async with websockets.connect(uri) as websocket:\n",
    "        await websocket.send(\"data request\")\n",
    "        response = await websocket.recv()\n",
    "        print(f\"Received: {response}\")\n",
    "\n",
    "asyncio.get_event_loop().run_until_complete(communicate())\n",
    "\n",
    "def plot_results(results):\n",
    "    plt.figure(figsize=(10, 6))\n",
    "    plt.hist(results, bins=20, color='blue', edgecolor='black')\n",
    "    plt.title('Results Distribution')\n",
    "    plt.xlabel('Results')\n",
    "    plt.ylabel('Frequency')\n",
    "    plt.show()\n",
    "\n",
    "# Örnek veriyi görselleştirin\n",
    "results = [d['result'] for d in data]\n",
    "plot_results(results)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "4412840f-232f-413c-bcdf-3b0b6b4d6998",
   "metadata": {},
   "outputs": [],
   "source": [
    "import time\n",
    "\n",
    "def autonomous_learning_cycle(interval=3600):\n",
    "    while True:\n",
    "        try:\n",
    "            # Örnek veri toplama\n",
    "            task = \"browse\"\n",
    "            url = \"https://example.com\"\n",
    "            result = ai_decision(task, url)\n",
    "            log_result(task, url, result)\n",
    "\n",
    "            # Model güncelleme\n",
    "            update_model()\n",
    "\n",
    "            # Geri bildirim toplama\n",
    "            feedback = collect_feedback(task, url, result)\n",
    "\n",
    "            if feedback:\n",
    "                print(\"Model updated based on feedback.\")\n",
    "\n",
    "            # Belirli bir süre bekle\n",
    "            time.sleep(interval)\n",
    "        except Exception as e:\n",
    "            print(f\"An error occurred: {e}\")\n",
    "\n",
    "# Otonom döngüyü başlat\n",
    "autonomous_learning_cycle()\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "4efcfdde-5866-42af-b2c1-69ad5c960351",
   "metadata": {},
   "outputs": [],
   "source": [
    "import pickle\n",
    "\n",
    "def save_model(model, filename='model.pkl'):\n",
    "    with open(filename, 'wb') as f:\n",
    "        pickle.dump(model, f)\n",
    "\n",
    "def load_model(filename='model.pkl'):\n",
    "    with open(filename, 'rb') as f:\n",
    "        return pickle.load(f)\n",
    "\n",
    "# Modeli kaydet\n",
    "save_model(model)\n",
    "\n",
    "# Modeli yükle\n",
    "loaded_model = load_model()\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "24188b44-239c-4e5b-b2ee-ab802cb2f88f",
   "metadata": {},
   "outputs": [],
   "source": [
    "import pandas as pd\n",
    "\n",
    "def save_data(data, filename='data.csv'):\n",
    "    df = pd.DataFrame(data)\n",
    "    df.to_csv(filename, index=False)\n",
    "\n",
    "def load_data(filename='data.csv'):\n",
    "    return pd.read_csv(filename).to_dict(orient='records')\n",
    "\n",
    "# Verileri kaydet\n",
    "save_data(data)\n",
    "\n",
    "# Verileri yükle\n",
    "loaded_data = load_data()\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "6a663926-c541-4519-95c7-08ca5aa4ca75",
   "metadata": {},
   "outputs": [],
   "source": [
    "import ipywidgets as widgets\n",
    "from IPython.display import display\n",
    "\n",
    "task_input = widgets.Dropdown(\n",
    "    options=['browse', 'news', 'images'],\n",
    "    value='browse',\n",
    "    description='Task:'\n",
    ")\n",
    "url_input = widgets.Text(\n",
    "    value='https://example.com',\n",
    "    description='URL:'\n",
    ")\n",
    "run_button = widgets.Button(description='Run')\n",
    "output_area = widgets.Output()\n",
    "\n",
    "def run_task(b):\n",
    "    task = task_input.value\n",
    "    url = url_input.value\n",
    "    result = ai_decision(task, url)\n",
    "    with output_area:\n",
    "        output_area.clear_output()\n",
    "        print(f\"Result for {task} on {url}: {result}\")\n",
    "\n",
    "run_button.on_click(run_task)\n",
    "\n",
    "display(task_input, url_input, run_button, output_area)\n"
   ]
  },
