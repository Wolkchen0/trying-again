   "source": [
    "# serverless.yml\n",
    "service: ai-model\n",
    "\n",
    "provider:\n",
    "  name: aws\n",
    "  runtime: python3.8\n",
    "\n",
    "functions:\n",
    "  predict:\n",
    "    handler: handler.predict\n",
    "    events:\n",
    "      - http:\n",
    "          path: predict\n",
    "          method: post\n"
