   "source": [
    "import requests\n",
    "from bs4 import BeautifulSoup\n",
    "\n",
    "def ai_decision(task, url):\n",
    "    task = task.strip().lower()  # Boşlukları sil ve küçük harfe çevir\n",
    "    if task == \"browse\":\n",
    "        return browse_internet(url)\n",
    "    elif task == \"news\":\n",
    "        return get_news_headlines(url)\n",
    "    else:\n",
    "        return \"Unrecognized task. Please enter 'browse' or 'news'.\"\n",
    "\n",
    "def browse_internet(url):\n",
    "    response = requests.get(url)\n",
    "    soup = BeautifulSoup(response.text, 'html.parser')\n",
    "    return soup.title.string\n",
    "\n",
    "def get_news_headlines(url):\n",
    "    response = requests.get(url)\n",
    "    soup = BeautifulSoup(response.text, 'html.parser')\n",
    "    headlines = soup.find_all('h2')\n",
    "    return [headline.get_text() for headline in headlines]\n",
    "\n",
    "# Kullanıcıdan görev ve URL al\n",
    "task = input(\"Enter task (e.g., browse, news): \")\n",
    "url = input(\"Enter URL to browse or get news from: \")\n",
    "result = ai_decision(task, url)\n",
    "print(\"Result:\", result)\n"
   ]
  },
  {
