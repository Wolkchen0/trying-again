  {
   "cell_type": "code",
   "execution_count": null,
   "id": "e8c0769b-3917-4460-8c11-ebf204c54786",
   "metadata": {},
   "outputs": [],
   "source": [
    "import time\n",
    "\n",
    "def autonomous_learning_cycle(interval=3600):\n",
    "    while True:\n",
    "        try:\n",
    "            # Veri toplama ve güncelleme\n",
    "            task = \"browse\"\n",
    "            url = \"https://example.com\"\n",
    "            result = ai_decision(task, url)\n",
    "            log_result(task, url, result)\n",
    "\n",
    "            # Modeli yeniden eğit\n",
    "            update_model()\n",
    "\n",
    "            # Geri bildirim toplama\n",
    "            feedback = collect_feedback(task, url, result)\n",
    "\n",
    "            # Modelin güncellenmesi\n",
    "            if feedback:\n",
    "                print(\"Model updated based on feedback.\")\n",
    "\n",
    "            # Belirli bir süre bekle\n",
    "            time.sleep(interval)\n",
    "        except Exception as e:\n",
    "            print(f\"An error occurred: {e}\")\n",
    "\n",
    "autonomous_learning_cycle()\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "8c3b5111-6e93-484d-a361-2c2edfe85406",
   "metadata": {},
   "outputs": [],
   "source": [
    "import nltk\n",
    "from nltk.corpus import stopwords\n",
    "from nltk.tokenize import word_tokenize\n",
    "\n",
    "nltk.download('punkt')\n",
    "nltk.download('stopwords')\n",
    "\n",
    "def analyze_input(text):\n",
    "    words = word_tokenize(text.lower())\n",
    "    filtered_words = [word for word in words if word.isalnum() and word not in stopwords.words('english')]\n",
    "    return filtered_words\n",
    "\n",
    "# Girdi analizi\n",
    "user_input = \"I want to browse the example site for news.\"\n",
    "analyzed_input = analyze_input(user_input)\n",
    "print(\"Analyzed Input:\", analyzed_input)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "06516658-2bfd-46c5-b3c9-e3c4ceca4370",
   "metadata": {},
   "outputs": [],
   "source": [
    "import spacy\n",
    "\n",
    "nlp = spacy.load(\"en_core_web_sm\")\n",
    "\n",
    "def extract_entities(text):\n",
    "    doc = nlp(text)\n",
    "    return [(ent.text, ent.label_) for ent in doc.ents]\n",
    "\n",
    "# Girdi analizi ve varlık çıkarma\n",
    "user_input = \"Find news on example.com about technology.\"\n",
    "entities = extract_entities(user_input)\n",
    "print(\"Extracted Entities:\", entities)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "61332cd5-d90e-4481-a388-4ad301231191",
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.metrics import accuracy_score\n",
    "\n",
    "def evaluate_model(model, X_test, y_test):\n",
    "    predictions = model.predict(X_test)\n",
    "    return accuracy_score(y_test, predictions)\n",
    "\n",
    "# Model değerlendirme örneği\n",
    "# X_test ve y_test test verileri olarak tanımlanmalıdır\n",
    "accuracy = evaluate_model(model, X_test, y_test)\n",
    "print(\"Model Accuracy:\", accuracy)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "a169e87f-cf80-42ad-b7c2-49a4369c1d20",
   "metadata": {},
   "outputs": [],
   "source": [
    "def update_model():\n",
    "    # Toplanan verileri kullanarak modeli yeniden eğit\n",
    "    texts = [d[\"url\"] for d in data]\n",
    "    results = [d[\"result\"] for d in data]\n",
    "    X = vectorizer.fit_transform(texts)\n",
    "    model.fit(X, results)\n",
    "\n",
    "    # Model performansını değerlendirin ve güncelleyin\n",
    "    accuracy = evaluate_model(model, X_test, y_test)\n",
    "    print(\"Updated Model Accuracy:\", accuracy)\n"
   ]
  },
