    "def collect_detailed_feedback(task, url, result):\n",
    "    print(f\"Evaluating task '{task}' for url '{url}'\")\n",
    "    rating = input(\"Rate the result on a scale from 1 to 5: \")\n",
    "    comments = input(\"Any additional comments? \")\n",
    "    return {\"rating\": rating, \"comments\": comments}\n",
    "\n",
    "feedback = collect_detailed_feedback(task, url, result)\n",
    "print(\"Detailed Feedback collected:\", feedback)\n"
