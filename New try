   "metadata": {},
   "outputs": [],
   "source": [
    "user_profiles = {}\n",
    "\n",
    "def update_user_profile(user_id, feedback):\n",
    "    if user_id not in user_profiles:\n",
    "        user_profiles[user_id] = {'positive': 0, 'negative': 0}\n",
    "    if feedback:\n",
    "        user_profiles[user_id]['positive'] += 1\n",
    "    else:\n",
    "        user_profiles[user_id]['negative'] += 1\n",
    "\n",
    "# Kullanıcı profili güncelleme örneği\n",
    "user_id = \"12345\"\n",
    "feedback = True  # Örnek pozitif geri bildirim\n",
    "update_user_profile(user_id, feedback)\n",
    "print(\"User Profiles:\", user_profiles)\n"
   ]
  },
