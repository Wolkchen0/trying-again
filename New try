    "import hashlib\n",
    "\n",
    "def anonymize_data(data):\n",
    "    anonymized_data = hashlib.sha256(data.encode()).hexdigest()\n",
    "    return anonymized_data\n",
    "\n",
    "# Kullanıcı verilerini anonimleştirme\n",
    "user_data = \"user@example.com\"\n",
    "anonymized_user_data = anonymize_data(user_data)\n",
    "print(\"Anonymized Data:\", anonymized_user_data)\n"
   ]
  },
