    "from sklearn.cluster import KMeans\n",
    "\n",
    "def analyze_user_profiles(user_profiles):\n",
    "    features = [[p['positive'], p['negative']] for p in user_profiles.values()]\n",
    "    kmeans = KMeans(n_clusters=2)\n",
